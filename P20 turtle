from turtle import *
import math
pensize(3)
pu()
bk(160)
pd()
fillcolor("red")
begin_fill()
circle(66)
end_fill()

#eyes
pu()
fd(160)
pd()
fillcolor("blue")
begin_fill()
circle(66)
end_fill()
#nose
pu()
bk(80)
rt(90)
fd(50)
lt(90)
pd()
fillcolor("green")
begin_fill()
circle(25)
end_fill()

#mouth
pu()
goto(-260, -50)
pd()
rt(90)
fd(50)
lt(90)
fd(350)
lt(90)
fd(50)

#outline
pu()
rt(90)
fd(30)
lt(90)
pd()
circle(250)
